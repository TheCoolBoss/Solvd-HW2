<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="client" type="client"/>

    <xs:complexType name="client">
      <xs:sequence>
        <xs:element name="id" type="xs:integer"/>
        <xs:element name="name" type="xs:string"/>
        <xs:element name="clientType">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="clientTypeId" type="xs:integer"/>
              <xs:element name="clientTypeName" type="xs:string"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>


</xs:schema>