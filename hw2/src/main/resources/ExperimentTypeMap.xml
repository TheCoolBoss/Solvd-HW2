<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ExperimentTypeMap.xml">
    <select id="getExperimentTypeById" parameterType="int" resultType="hashmap">
        select * from experimentTypes where experimentTypeId = #{id}
    </select>

    <delete id="deleteExperimentType" parameterType="int">
        delete from experimentTypes where experimentTypeId = #{experimentTypeId}
    </delete>
</mapper>