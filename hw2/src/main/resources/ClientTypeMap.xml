<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ClientTypeMap.xml">
    <select id="getClientTypeById" parameterType="int" resultType="com.solvd.hw2.models.ClientType">
        select clientTypeId, clientTypeName from clientTypes where clientTypeId = #{id}
    </select>

    <delete id="deleteClientType" parameterType="int">
        delete from clientTypes where clientTypeId = #{clientTypeId}
    </delete>
</mapper>